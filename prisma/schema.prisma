// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" // Change to "env("DATABASE_URL")" for production
  // shadowDatabaseUrl = env("SHADOW_DATABASE_URL") // Comment in for sqlserver in production
}

model Contact {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  first     String?
  last      String?
  avatar    String?
  email     String?
  twitter   String?
  favorite  Boolean  @default(false)
  notes     String?
}
